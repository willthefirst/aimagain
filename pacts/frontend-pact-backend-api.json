{
  "consumer": {
    "name": "frontend-pact"
  },
  "provider": {
    "name": "backend-api"
  },
  "interactions": [
    {
      "description": "a request to register a new user via web form",
      "providerState": "User test.user@example.com does not exist",
      "request": {
        "method": "POST",
        "path": "/auth/register",
        "headers": {
          "Content-Type": "application/x-www-form-urlencoded"
        },
        "body": "email=test.user%40example.com&password=securepassword123&username=testuser",
        "matchingRules": {
          "$.body.email": {
            "match": "type"
          },
          "$.body.password": {
            "match": "type"
          },
          "$.body.username": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 201,
        "headers": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}